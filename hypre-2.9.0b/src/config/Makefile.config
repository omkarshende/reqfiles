HYPRE_INSTALL_DIR = NONE
HYPRE_LIB_INSTALL = ${exec_prefix}/lib
HYPRE_INC_INSTALL = ${prefix}/include

HYPRE_LIB_SUFFIX = .a

.SUFFIXES:
.SUFFIXES: .o .f .c .C .cxx .cc

.f.o:
	$(F77) $(FFLAGS) -c $< 
.c.o:
	$(CC) $(CFLAGS) -c $< 
.C.o:
	$(CXX) $(CXXFLAGS) -c $<
.cxx.o:
	$(CXX) $(CXXFLAGS) -c $<
.cc.o:
	$(CXX) $(CXXFLAGS) -c $<

F77       = mpiifort
FFLAGS    = -O2  $(F77_COMPILE_FLAGS)

CC        = mpiicc
CFLAGS    = -O2 -DHAVE_CONFIG_H $(C_COMPILE_FLAGS)

CXX       = mpiicpc
CXXFLAGS  = -O2 -DHAVE_CONFIG_H $(CXX_COMPILE_FLAGS)

LINK_F77  = mpiifort
LINK_CC   = mpiicc
LINK_CXX  = mpiicpc

BUILD_F77_SHARED    = 
BUILD_CC_SHARED     = 
BUILD_CXX_SHARED    = 
